<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cancer_track">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hearticon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/hearticon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".InfoActivity" />
        <activity android:name=".QuestionActivity" />
        <activity android:name=".SymptomActivity" />
        <activity android:name=".MedicationActivity" />
        <activity android:name=".BookingActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
    </application>

    <uses-permission android:name="android.permission.CALL_PHONE" />

</manifest>